<%= simple_form_for(@recette) do |f| %>
  <%= f.input :title %>
  <%= f.input :contenu %>
  <%= f.input :intro %>
  <%= f.input :categorie %>
  <%= f.input :redirection %>
  <%= f.input :photo, as: :file,class: "input-photo" %>
  <%= f.input :photo_2, as: :file,class: "input-photo" %>
  <%= f.submit class: "btn btn-primary" %>
<% end %>
<%= link_to "retour", recettes_path %>
